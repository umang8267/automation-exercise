<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MySuite" parallel="tests" thread-count="3">
		<listeners>
		<listener class-name="Unitity.ExtentManager"></listener>
	</listeners>

	<parameter name="browser" value="chrome" />
    <test name="Product adding to cart">
        <classes>
           <class name="Testcases.ProductAddInCart"/>
        </classes>
    </test>

    <test name="User SignUp functionality">
        <classes>
             <class name="Testcases.UserSignUP"/>
        </classes>
    </test>
       <test name="UserLogin Functionlaity">
        <classes>
             <class name="Testcases.UserLogin"/>
        </classes>
    </test>
</suite>